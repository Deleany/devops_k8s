#!groovy
// Check properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '3', artifactNumToKeepStr: '3'))
        timestamps()
    }
    stages {
        stage("login to k8s") {
            steps {
                echo $GIT_COMMIT
                sh 'ssh root@192.168.88.200 \
\'kubectl get nodes\''
            }
        }
    }
}
